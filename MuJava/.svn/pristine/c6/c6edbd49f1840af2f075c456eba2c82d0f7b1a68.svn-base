package mujava.plugin.wizards.generation;

public class ParamState {
	private int index = -1;
	private String name = "";
	private String type = "";

	public int getIndex() {
		return index;
	}

	public String getParamName() {
		return this.name;
	}

	public String getType() {
		return this.type;
	}

	public void setIndex(int index) {
		if (index >= 0)
			this.index = index;
		else
			this.index = -1;
	}

	public void setParamName(String name) {
		if (name != null)
			this.name = name;
	}

	public void setType(String type) {
		if (type != null)
			this.type = type;
	}
}
