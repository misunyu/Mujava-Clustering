package MSG.traditional;

import MSG.MSGConstraints;

public class BORMetaMutant {
	
	public static int BOR(byte left, byte right, int op, String id) {
		int original = left;

		if (op == MSGConstraints.B_BITAND)
			original = left & right;
		if (op == MSGConstraints.B_BITOR)
			original = left | right;
		if (op == MSGConstraints.B_XOR)
			original = left ^ right;
		if (op == MSGConstraints.B_SHIFT_L)
			original = left << right;
		if (op == MSGConstraints.B_SHIFT_R)
			original = left >> right;
		if (op == MSGConstraints.B_SHIFT_RR)
			original = left >>> right;

		return original;
	}
	
